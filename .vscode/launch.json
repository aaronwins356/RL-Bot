{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "RLBot GUI: Launch via rlbot.cfg",
      "type": "python",
      "request": "launch",
      "module": "rlbot",
      "args": ["--rlbot-config", "${workspaceFolder}/rlbot.cfg"],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "Agent: single instance",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}
